doctype html

block vars

head
    title= title || "Sensitive Question"
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    meta(name="mobile-web-app-capable" content="yes")
    
    link(rel="stylesheet", href="main.css")
    - if(settings.env == "development")
        link(rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/angular_material/1.1.0-rc2/angular-material.css")
        script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.3/angular.js")
        script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.3/angular-animate.js")
        script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.3/angular-aria.js")
        script(src="//ajax.googleapis.com/ajax/libs/angular_material/1.1.0-rc2/angular-material.js") 
    - else
        link(rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/angular_material/1.1.0-rc2/angular-material.min.css")
        script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.3/angular.min.js")
        script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.3/angular-animate.min.js")
        script(src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.3/angular-aria.min.js")
        script(src="//ajax.googleapis.com/ajax/libs/angular_material/1.1.0-rc2/angular-material.min.js") 
    script(src="//cdnjs.cloudflare.com/ajax/libs/angular-material-icons/0.7.1/angular-material-icons.min.js")
    
    block head

body
    block body

    